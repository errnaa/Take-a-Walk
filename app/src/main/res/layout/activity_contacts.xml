<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="taw.takeawalk.ContactsActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Emergency Contacts"
        android:id="@+id/textView10"
        android:textSize="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Message All"
        android:id="@+id/MessageButton"
        android:textSize="25dp"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/textView10"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Zhang"
        android:id="@+id/textView11"
        android:textSize="30dp"
        android:layout_marginTop="38dp"
        android:layout_below="@+id/MessageButton"
        android:layout_alignLeft="@+id/textView10"
        android:layout_alignStart="@+id/textView10" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/call1"
        android:background="#26e723"
        android:layout_alignTop="@+id/textView11"
        android:layout_toLeftOf="@+id/Text1"
        android:layout_toStartOf="@+id/Text1"
        android:layout_marginRight="30dp"
        android:layout_marginEnd="30dp"
        android:layout_alignBottom="@+id/textView11" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:id="@+id/Text1"
        android:background="#6db9e2"
        android:layout_toRightOf="@+id/MessageButton"
        android:layout_toEndOf="@+id/MessageButton"
        android:layout_alignTop="@+id/call1"
        android:layout_alignBottom="@+id/call1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Peter"
        android:id="@+id/textView12"
        android:textSize="30dp"
        android:layout_below="@+id/textView11"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="30dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/Call2"
        android:background="#26e723"
        android:layout_alignTop="@+id/textView12"
        android:layout_alignLeft="@+id/call1"
        android:layout_alignStart="@+id/call1"
        android:layout_alignBottom="@+id/textView12" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:id="@+id/Text2"
        android:background="#6db9e2"
        android:layout_alignLeft="@+id/Text1"
        android:layout_alignStart="@+id/Text1"
        android:layout_alignTop="@+id/Call2"
        android:layout_alignBottom="@+id/Call2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Dad"
        android:id="@+id/textView13"
        android:textSize="30dp"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/textView12"
        android:layout_alignLeft="@+id/textView12"
        android:layout_alignStart="@+id/textView12" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/Call3"
        android:background="#26e723"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignLeft="@+id/Call2"
        android:layout_alignStart="@+id/Call2"
        android:layout_alignTop="@+id/textView13" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Text"
        android:id="@+id/Text3"
        android:background="#6db9e2"
        android:layout_alignBottom="@+id/Call3"
        android:layout_alignLeft="@+id/Text2"
        android:layout_alignStart="@+id/Text2"
        android:layout_alignTop="@+id/Call3" />
</RelativeLayout>
